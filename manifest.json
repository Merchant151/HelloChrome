{
	"manifest_version":3,
	"name": "HelloChrome",
	"description": "Hello World Chrome extension",
	"version": "0.0.1",
	"icons": {
		"16":"/images/HUC16.png",
		"48": "/images/HUC48.png",
		"128": "/images/HUC128.png"
	},
	"action": {
		"default_icon": {
			"16": "/images/HUC16.png",
			"48": "/images/HUC48.png",
			"128": "/images/HUC128.png"
		}
	},
	"content_scripts": [{
		"matches": ["https://google.com/*"],
		"matches": ["<all_urls>"],
		"css":["main.css"]
	}]
}
